def postup():
	vystup = ['1']
 	for i in range(1, 50):			
		delitelnost(i, vystup)
		if len(vystup) < i:
			vystup.append(i)

	return vystup

	
def delitelnost(x, p):
	vypis = []
	vstup = [3, 7, 11, 13]
	slovo = ['tri', 'sedem', 'jedenast', 'trinast']
	for n in range(len(vstup)):	
		if x % vstup[n] == 0:
			vypis.append(slovo[n])
	
	if len(vypis) > 0:
		p.append(vypis)
			

			
print postup()
