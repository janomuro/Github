def postup():
	vystup = []
	i = 1
 	while i < 15:			
		delitelnost(i, vystup)
		
		print "i", i
		print "dlzka vystup", len(vystup)
		if len(vystup) < i:
			vystup.append(i)

 		i = i + 1
	return vystup

	
def delitelnost(x, p):
	vstup = [3, 7, 11, 13]
	slovo = ['tri', 'sedem', 'jedenast', 'trinast']
	for n in range(len(vstup)):	
		if x % vstup[n] == 0:
			p.append(slovo[n])
			

			
print postup()
